<Window x:Class="Nfh.Editor.MetaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nfh.Editor" 
        xmlns:views="clr-namespace:Nfh.Editor.Views" xmlns:viewmodels="clr-namespace:Nfh.Editor.ViewModels"
        mc:Ignorable="d"
        Title="Level Metadata Editor" 
        Width="850" Height="650"
        Closing="Window_Closing"
        Closed="Window_Closed">
    <views:MetaView x:Name="metaView">
        <views:MetaView.DataContext>
            <viewmodels:MetaViewModel/>
        </views:MetaView.DataContext>
    </views:MetaView>
</Window>
