<Window x:Class="Nfh.Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nfh.Editor" 
        xmlns:thumbs="clr-namespace:Nfh.Editor.Thumbs" 
        xmlns:views="clr-namespace:Nfh.Editor.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        x:Name="mainWindow">
    <Window.Resources>
    </Window.Resources>
    <DockPanel>
        <views:SeasonPackView DataContext="{Binding RelativeSource={RelativeSource AncestorType={x:Type Window}}, Path=SeasonPack}">
        </views:SeasonPackView>
    </DockPanel>
</Window>
